{
  "presets": [
    ["@babel/env", { "modules": false, "targets": { "node": "10.13.0" } }],
    "@babel/preset-react"
  ],
  "plugins": ["@babel/plugin-transform-runtime"],
  "overrides": [
    {
      "test": ["**/*.ts", "**/*.tsx"],
      "plugins": [["@babel/plugin-transform-typescript", { "isTSX": true }]]
    }
  ]
}
